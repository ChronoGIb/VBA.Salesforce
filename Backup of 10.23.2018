If EngagementID = "" Then
PAXn = fPAX
fPAX = ""
Else
PAXn = PAHR_CollectInfo(EngagementID)
End If
'Call Print_L2_HTML(PAXn)

'make sure the engagement should even be proccessed for housing
    Eligibility = True

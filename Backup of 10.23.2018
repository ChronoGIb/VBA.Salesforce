            Table.Add "</td>"
            If UBound(DataModel) = 1 Then
                Table.Add "<td>" & DataModel(1) & "</td>"
            End If
            Table.Add "</tr></table>"
        DataModel = ""
        End If
    End If

Set InfoPrinter_Cutter = Table

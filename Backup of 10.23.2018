    If Street(2)(i)(0) <> "" Then
        tString = tString & " " & Street(2)(i)(2)(0)
    End If
Next i
tString = Trim(tString)
Street(0) = tString

Address = PageInfo_RR(Address, "Street", Street)

End Sub
